package zh0307;

import java.util.ArrayList;

public class ArrayListTest {
	public static void main(String[] args) {
		ArrayList<String> st = new ArrayList<String>();
		st.add("aaa");
		st.add("bbb");
		st.add("ccc");
		st.add("ddd");
		
		st.remove(0);
		
		for (int i = 0; i < st.length; i++) {
			System.out.println(st[i]);
		}
		
		
	}
}
