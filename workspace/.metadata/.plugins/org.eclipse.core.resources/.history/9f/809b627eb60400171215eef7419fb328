package o20170309;

public class In4ReflectObj {
	public static void main(String[] args) {
		Class<?> c = null;
		In3Student st = null;
		c = In3Student.class;
		
		try {
			st = (In3Student)c.newInstance();
		} catch (InstantiationException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		} catch (IllegalAccessException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
				
	}
}
