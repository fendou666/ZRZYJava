package JD20170331;

import java.io.File;
import java.io.FileInputStream;

public class J1 {
	public static boolean readFile(File f){
		if(!f.exists()){
			return false;
		}
		FileInputStream fis = new FileInputStream(f);
		
		
	}
	
	
	
	public static void main(String[] args) {
		File f = new File();
		readFile(f);
	}
}
