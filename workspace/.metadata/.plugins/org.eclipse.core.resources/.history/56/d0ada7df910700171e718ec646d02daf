package com.chinasoft.po;

public class Flower {
	private int number;
	private String name;
	private int count;
	private double price;
	
	public Flower() {
		super();
	}

	public Flower(int number, String name, int count, double price) {
		super();
		this.number = number;
		this.name = name;
		this.count = count;
		this.price = price;
	}
	
	
	
	
}
