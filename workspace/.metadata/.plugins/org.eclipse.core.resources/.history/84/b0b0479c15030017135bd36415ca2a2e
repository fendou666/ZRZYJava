package o20170307Thread;

public class In1Thread extends Thread {
	private int tickerToutle = 20;
	private String name;
	
	public In1Thread(String name){
		this.name = name;
	}
	
	public void run(){
		// 因为线程的原因不使用i，为了看线程共享数据的问题
//		for(int i=0;i<tickerToutle;i++) {
//			System.out.println(name + "卖出第" +i + "张票");
//		}
		for(int i=0;i<tickerToutle;i++) {
			System.out.println(name + "卖出第" +i + "张票");
		}
	}
	
}
