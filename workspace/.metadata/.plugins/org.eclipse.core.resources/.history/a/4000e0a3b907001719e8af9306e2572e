package com.test;

public class A2Teacher extends A1People {
	private double salary;

	
	
	public A2Teacher() {
		super();
		// TODO Auto-generated constructor stub
	}


	public A2Teacher(String name, int age) {
		super(name, age);
		// TODO Auto-generated constructor stub
	}

	public A2Teacher(String name, int age, int sno) {
		super(name, age);
		this.sno = sno;
	}
	
	public String PrintInfo() {
		return "老师姓名：" + getName() + ", 年龄：" + getAge() + "， 工资：" + sno ;
	}
	
	public static void main(String[] args) {
		A2Student stu = new A2Student("小明", 10, 22123);
		System.out.println(stu.PrintInfo());
	}
	
	
}
