package SerializableT;

public class A3Circulation {
//	1到10求和
	public void getSum(){
		int sum = 0; // 设置循环中需要的变量
		int i = 1; // 定义条件变量
		while(i<=10){ // 设置退出条件
			sum = sum +i;
			i++; // 条件步进
		}
		System.out.println(sum);
		
		sum = 0;
		i = 0;
		do {
			sum = sum +i;
			
			i++;
		}while(i<=10);
		
		
	}
	
//	1到100之间7的倍数输出
	public void getMultiple(){
		
	}
	
	
//	打印圣诞树
}
